# ملف .env.example
# يجب إعادة تسمية هذا الملف إلى .env وملء القيم قبل التشغيل

# مفتاح API لرفع صور QR Code (اختياري)
IMGBB_KEY="YOUR_IMGBB_API_KEY"

# رابط الاتصال بقاعدة بيانات MongoDB (ضروري لحفظ البيانات)
# يمكنك الحصول على رابط مجاني من MongoDB Atlas
MONGO_URI="mongodb+srv://<username>:<password>@<cluster-name>.mongodb.net/kede_bot?retryWrites=true&w=majority"

# المنفذ الذي سيعمل عليه خادم Uptime (Render سيقوم بتعيينه تلقائياً)
PORT=3000
